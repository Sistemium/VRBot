{
  "name": "vr-bot",
  "version": "1.0.0",
  "description": "Vse Ramki Telegram bot",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src --fix",
    "test": "npm run lint",
    "start": "nodemon --exec 'npm run lint && node -r esm' -- src/index.js",
    "api": "nodemon --exec 'npm run lint && node -r esm' -- src/api",
    "pkg": "cp package.json dist",
    "build": "npm run lint && rm -rf dist && cp -r src dist && npm run pkg"
  },
  "dependencies": {
    "@koa/cors": "^2.2.3",
    "async": "^2.6.1",
    "aws-sdk": "^2.321.0",
    "axios": "^0.18.0",
    "date-fns": "v2.0.0-alpha.8",
    "esm": "^3.2.14",
    "koa": "^2.7.0",
    "koa-body": "^4.1.0",
    "koa-bodyparser": "^4.2.1",
    "koa-morgan": "^1.0.1",
    "koa-router": "^7.4.0",
    "lodash": "^4.17.10",
    "mongoose": "^5.4.17",
    "node-xlsx": "^0.12.1",
    "sharp": "^0.30.5",
    "sistemium-telegram": "^1.2.11",
    "telegraf": "^3.24.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-plugin-import": "^2.13.0",
    "nodemon": "^1.18.3"
  },
  "repository": {
    "type": "git",
